{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.27",
        "barryvdh/laravel-debugbar": "^3.1",
        "dingo/api": "2.0.0-alpha2",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "~3.3",
        "gregwar/captcha": "1.*",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "laravel-notification-channels/webpush": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "~4.0",
        "laravel/scout": "^4.0",
        "laravel/tinker": "~1.0",
        "liyu/dingo-serializer-switch": "^0.3.0",
        "overtrue/easy-sms": "^1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "socialiteproviders/weixin": "^4.0",
        "spatie/laravel-mailable-test": "^2.0",
        "summerblue/administrator": "^1.0",
        "tom-lingham/searchy": "2.*",
        "toplan/laravel-sms": "~2.6",
        "tymon/jwt-auth": "1.0.0-rc.2",
        "upyun/sdk": "^2.2",
        "yangyifan/upload": "v0.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
